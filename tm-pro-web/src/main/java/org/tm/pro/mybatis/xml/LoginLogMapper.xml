<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tm.pro.mapper.LoginLogDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.tm.pro.entity.LoginLog">
        <id column="id" property="id" />
        <result column="login_name" property="loginName" />
        <result column="login_pass" property="loginPass" />
        <result column="operating_system" property="operatingSystem" />
        <result column="browser" property="browser" />
        <result column="browser_version" property="browserVersion" />
        <result column="login_result" property="loginResult" />
        <result column="login_time" property="loginTime" />
    </resultMap>

</mapper>
